{
  "name": "apollo-client",
  "version": "0.3.9",
  "description": "A simple yet functional GraphQL client.",
  "main": "./lib/src/index.js",
  "typings": "./lib/src/index.d.ts",
  "scripts": {
    "deploy": "rm -rf ./lib && npm run compile && ./scripts/deploy.sh",
    "pretest": "npm run compile",
    "test": "npm run testonly --",
    "posttest": "npm run lint",
    "filesize": "npm run compile:browser && ./scripts/filesize.js --file=./dist/index.min.js --maxGzip=31",
    "compile": "tsc",
    "compile:browser": "rm -rf ./dist && mkdir ./dist && browserify ./lib/src/index.js -o=./dist/index.js && npm run minify:browser",
    "minify:browser": "uglifyjs --compress --mangle --screw-ie8 -o=./dist/index.min.js -- ./dist/index.js",
    "watch": "tsc -w",
    "prepublish": "npm run compile",
    "lint": "grunt tslint",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter dot --full-trace lib/test/tests.js",
    "postcoverage": "remap-istanbul --input coverage/coverage.json --type lcovonly --output coverage/lcov.info",
    "testonly": "mocha --reporter spec --full-trace lib/test/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "apollostack/apollo-client"
  },
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^3.1.2",
    "graphql": "^0.4.18 || ^0.5.0",
    "lodash.assign": "^4.0.8",
    "lodash.forown": "^4.1.0",
    "lodash.has": "^4.3.1",
    "lodash.includes": "^4.1.2",
    "lodash.isarray": "^4.0.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.isnull": "^3.0.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "redux": "^3.3.1",
    "symbol-observable": "^0.2.4"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "colors": "^1.1.2",
    "dataloader": "^1.1.0",
    "es6-promise": "^3.1.2",
    "grunt": "1.0.1",
    "grunt-tslint": "3.1.0",
    "gzip-size": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.2",
    "lodash": "^4.6.1",
    "lodash.mapvalues": "^4.3.0",
    "lodash.omit": "^4.2.1",
    "minimist": "^1.2.0",
    "mocha": "^2.3.3",
    "pretty-bytes": "^3.0.1",
    "remap-istanbul": "^0.5.1",
    "request-promise": "^2.0.1",
    "source-map-support": "^0.4.0",
    "swapi-graphql": "^0.0.4",
    "tslint": "3.7.3",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "uglify-js": "^2.6.2",
    "rxjs": "^5.0.0-beta.7"
  }
}
