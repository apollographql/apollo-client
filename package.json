{
  "name": "apollo-client",
  "version": "0.0.3",
  "description": "A simple yet functional GraphQL client.",
  "main": "./lib/src/index.js",
  "scripts": {
    "pretest": "npm run compile",
    "test": "mocha --reporter spec --full-trace lib/test/tests.js",
    "posttest": "npm run lint && NODE_ENV=production gulp",
    "compile": "tsc",
    "watch": "tsc -w",
    "prepublish": "npm run compile",
    "lint": "tslint src/*.ts && tslint test/*.ts",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter dot --full-trace lib/test/tests.js",
    "postcoverage": "remap-istanbul --input coverage/coverage.json --type lcovonly --output coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "apollostack/apollo-client"
  },
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^3.1.2",
    "graphql": "^0.4.18",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.6.1",
    "lodash.assign": "^4.0.7",
    "lodash.forown": "^4.1.0",
    "lodash.has": "^4.3.1",
    "lodash.includes": "^4.1.2",
    "lodash.isarray": "^4.0.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.isnull": "^3.0.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "dataloader": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-browserify": "^0.5.1",
    "gulp-filesize": "0.0.6",
    "gulp-sizereport": "^1.1.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.3",
    "remap-istanbul": "^0.5.1",
    "request-promise": "^2.0.1",
    "rollup": "^0.25.8",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "source-map-support": "^0.4.0",
    "swapi-graphql": "^0.0.4",
    "tslint": "^3.6.0",
    "typescript": "^1.8.9",
    "typescript-require": "^0.2.9-1",
    "typings": "^0.7.9",
    "uglify-js": "^2.6.2"
  }
}
