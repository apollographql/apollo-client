{
  "bracketSpacing": true,
  "printWidth": 80,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "experimentalTernaries": true,
  "plugins": ["./config/prettier/format-jsdoc.js"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "options": {
        "parser": "typescript-with-jsdoc"
      }
    }
  ]
}
