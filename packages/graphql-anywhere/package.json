{
  "name": "graphql-anywhere",
  "version": "4.0.0",
  "description": "Run GraphQL queries with no schema and just one resolver",
  "main": "./lib/src/index.js",
  "typings": "./lib/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "posttest": "npm run lint && npm run compile",
    "compile": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run compile",
    "lint": "grunt tslint",
    "compile:browser": "browserify ./lib/src/index.js -o=./lib/index.js --i apollo-utilities && npm run minify:browser",
    "minify:browser": "uglifyjs -c -m -o ./lib/index.min.js -- ./lib/index.js",
    "filesize": "npm run compile:browser"
  },
  "repository": {
    "type": "git",
    "url": "apollographql/graphql-anywhere"
  },
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-utilities": "*"
  },
  "devDependencies": {
    "@types/graphql": "~0.9.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "^20.0.7",
    "@types/lodash": "^4.14.37",
    "@types/node": "^8.0.0",
    "@types/react": "^15.0.14",
    "@types/react-dom": "^15.5.0",
    "async": "^2.0.0",
    "browserify": "^14.4.0",
    "es6-promise": "^4.0.5",
    "graphql": "^0.10.0",
    "graphql-tag": "^2.0.0",
    "grunt": "1.0.1",
    "grunt-tslint": "5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "ts-jest": "^20.0.10",
    "tslint": "^5.5.0",
    "typescript": "^2.2.1",
    "uglify": "^0.1.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json"]
  }
}
