{
  "name": "tree-shaking-rollup-ac3",
  "version": "0.1.0",
  "description": "Apollo Client 3 bundling example app (rollup + tree-shaking).",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/apollographql/apollo-client.git"
  },
  "keywords": [
    "apollo",
    "graphql",
    "react"
  ],
  "author": "opensource@apollographql.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apollographql/apollo-client/issues"
  },
  "homepage": "https://github.com/apollographql/apollo-client#readme",
  "main": "./public/js/index.js",
  "module": "./src/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npx rollup -c",
    "clean": "npx rimraf ./public/js/*",
    "start": "npm run build && npx serve public",
    "start:no-compression": "npm run build && npx serve --no-compression public",
    "analyze": "npm run build && source-map-explorer public/js/app-no-react.min.js"
  },
  "dependencies": {
    "@apollo/client": "file:../../../../dist",
    "graphql": "^15.0.0",
    "react": "file:../../../../node_modules/react",
    "react-dom": "file:../../../../node_modules/react-dom"
  },
  "devDependencies": {
    "@babel/preset-react": "7.14.5",
    "rimraf": "3.0.2",
    "rollup": "1.32.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-gzip": "2.5.0",
    "rollup-plugin-jsx": "1.0.3",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "7.0.2",
    "serve": "12.0.0",
    "source-map-explorer": "2.5.2"
  }
}
