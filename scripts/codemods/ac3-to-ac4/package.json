{
  "name": "@apollo/client-codemod-migrate-3-to-4",
  "version": "1.0.0-rc.0",
  "description": "Apollo Client Codemod to migrate from version 3 to version 4",
  "keywords": [
    "apollo",
    "client",
    "graphql",
    "codemod"
  ],
  "author": "packages@apollographql.com",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollographql/apollo-client.git",
    "directory": "codegen"
  },
  "bugs": {
    "url": "https://github.com/apollographql/apollo-client/issues"
  },
  "homepage": "https://www.apollographql.com/docs/react/",
  "exports": {
    ".": "./dist/index.js",
    "./imports": "./dist/imports.js"
  },
  "files": [
    "dist",
    "README.md",
    ".codemodrc.json"
  ],
  "scripts": {
    "prebuild": "npm run clean; jq '.version=inputs.version' .codemodrc.json package.json > .codemodrc.json_; mv .codemodrc.json_ .codemodrc.json",
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "prepack": "npm run build",
    "publint": "publint run --strict .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "jscodeshift": "17.3.0"
  },
  "devDependencies": {
    "@codemod.com/codemod-utils": "*",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "20.9.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.1"
  }
}
