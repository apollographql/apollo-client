{
  "name": "@apollo/client",
  "version": "4.0.0-alpha.19",
  "description": "A fully-featured caching GraphQL client.",
  "private": true,
  "keywords": [
    "apollo",
    "graphql",
    "react",
    "hooks",
    "client",
    "cache"
  ],
  "author": "packages@apollographql.com",
  "license": "MIT",
  "type": "module",
  "main": "./dist/__cjs/core/index.cjs",
  "module": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "sideEffects": false,
  "react-native": {
    "react-dom/server": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollographql/apollo-client.git"
  },
  "bugs": {
    "url": "https://github.com/apollographql/apollo-client/issues"
  },
  "homepage": "https://www.apollographql.com/docs/react/",
  "exports": {
    ".": "./src/core/index.ts",
    "./cache": "./src/cache/index.ts",
    "./core": "./src/core/index.ts",
    "./dev": "./src/dev/index.ts",
    "./errors": "./src/errors/index.ts",
    "./link": "./src/link/index.ts",
    "./link/batch": "./src/link/batch/index.ts",
    "./link/batch-http": "./src/link/batch-http/index.ts",
    "./link/context": "./src/link/context/index.ts",
    "./link/error": "./src/link/error/index.ts",
    "./link/http": "./src/link/http/index.ts",
    "./link/persisted-queries": "./src/link/persisted-queries/index.ts",
    "./link/retry": "./src/link/retry/index.ts",
    "./link/remove-typename": "./src/link/remove-typename/index.ts",
    "./link/schema": "./src/link/schema/index.ts",
    "./link/subscriptions": "./src/link/subscriptions/index.ts",
    "./link/utils": "./src/link/utils/index.ts",
    "./link/ws": "./src/link/ws/index.ts",
    "./local-state": "./src/local-state/index.ts",
    "./masking": "./src/masking/index.ts",
    "./react": {
      "react-server": "./src/react/index.react-server.ts",
      "default": "./src/react/index.ts"
    },
    "./react/internal": "./src/react/internal/index.ts",
    "./react/ssr": "./src/react/ssr/index.ts",
    "./testing": "./src/testing/index.ts",
    "./testing/react": "./src/testing/react/index.ts",
    "./utilities": "./src/utilities/index.ts",
    "./utilities/internal": {
      "production": "./src/utilities/internal/index.production.ts",
      "default": "./src/utilities/internal/index.ts"
    },
    "./utilities/internal/globals": "./src/utilities/internal/globals/index.ts",
    "./utilities/subscriptions/relay": "./src/utilities/subscriptions/relay/index.ts",
    "./utilities/invariant": {
      "development": "./src/utilities/invariant/index.development.ts",
      "default": "./src/utilities/invariant/index.ts"
    },
    "./utilities/environment": {
      "production": "./src/utilities/environment/index.production.ts",
      "development": "./src/utilities/environment/index.development.ts",
      "default": "./src/utilities/environment/index.ts"
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "node config/build.ts",
    "typecheck": "tsc --project config/tsconfig.json; tsc --noEmit --project tsconfig.json",
    "postinstall": "patch-package",
    "extract-api": "npm run clean && node config/build.ts --step=prepareDist --step=addExports --step=typescript --step=inlineInheritDoc --step=deprecateInternals && node config/apiExtractor.ts --generate apiReport",
    "clean": "rimraf dist coverage lib temp",
    "check:format": "prettier --check .",
    "ci:precheck": "node config/precheck.ts",
    "format": "prettier --write .",
    "lint": "EXTENDED_RULES=1 eslint 'src/**/*.{[jt]s,[jt]sx}'",
    "test": "node --expose-gc --experimental-import-meta-resolve --disable-warning=ExperimentalWarning ./node_modules/jest/bin/jest.js --config ./config/jest.config.ts",
    "test:type-benches": "find src -name '*.bench.ts' | xargs -n1 tsx",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config ./config/jest.config.ts --runInBand --testTimeout 99999 --logHeapUsage",
    "test:ci": "TEST_ENV=ci npm run test:coverage -- --logHeapUsage",
    "test:codegen": "npm run build -w codegen && graphql-codegen --config ./tests.codegen.ts",
    "test:watch": "jest --config ./config/jest.config.ts --watch",
    "test:memory": "npm i && npm run build && cd scripts/memory && npm i && npm test",
    "test:coverage": "npm run coverage -- --ci --runInBand --reporters=default --reporters=jest-junit",
    "coverage": "node --experimental-import-meta-resolve --no-compilation-cache --expose-gc --max_old_space_size=4096 node_modules/.bin/jest --config ./config/jest.config.ts --logHeapUsage --coverage --watchAll=false",
    "bundlesize": "npm run build && node ./config/bundlesize.ts",
    "deploy": "cd dist && npm publish --tag next",
    "typedoc": "typedoc src/core/index.ts --json docs/public/docs.json",
    "docmodel": "node config/build.ts --step=typescript --step=inlineInheritDoc && node config/apiExtractor.ts --main-only --generate docModel",
    "changeset-check": "changeset status --verbose --since=origin/main",
    "changeset-publish": "npm pkg set 'workspaces[0]'=dist && npm run build && changeset publish",
    "changeset-version": "changeset version && npm i",
    "update-size-limits": "size-limit --json | jq '. | map(select(.sizeLimit) | { key: .name, value: .size}) | from_entries' | tee .new-size-limits.json; mv .new-size-limits.json .size-limits.json",
    "knip": "knip",
    "madge": "node ./config/madge.ts",
    "publint": "publint run --strict dist"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "error",
      "version": ">=23.6.0"
    },
    "packageManager": {
      "name": "npm",
      "onFail": "error",
      "version": ">=10.8.2"
    }
  },
  "peerDependencies": {
    "graphql": "^16.0.0",
    "graphql-ws": "^5.5.5 || ^6.0.3",
    "react": "^17.0.0 || ^18.0.0 || >=19.0.0-rc",
    "react-dom": "^17.0.0 || ^18.0.0 || >=19.0.0-rc",
    "rxjs": "^7.8.0",
    "subscriptions-transport-ws": "^0.9.0 || ^0.11.0"
  },
  "peerDependenciesMeta": {
    "graphql-ws": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "subscriptions-transport-ws": {
      "optional": true
    }
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.1.1",
    "@wry/caches": "^1.0.0",
    "@wry/equality": "^0.5.6",
    "@wry/trie": "^0.5.0",
    "graphql-tag": "^2.12.6",
    "optimism": "^0.18.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@actions/github-script": "github:actions/github-script#v7.0.1",
    "@arethetypeswrong/cli": "0.15.3",
    "@ark/attest": "0.28.0",
    "@babel/core": "^7.26.9",
    "@babel/parser": "7.25.0",
    "@babel/preset-env": "^7.26.9",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.7",
    "@eslint/compat": "1.2.5",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-tools/schema": "10.0.4",
    "@jest/expect-utils": "29.7.0",
    "@jest/globals": "29.7.0",
    "@microsoft/api-extractor": "7.49.1",
    "@microsoft/api-extractor-model": "7.30.2",
    "@microsoft/tsdoc": "0.15.1",
    "@size-limit/esbuild-why": "11.1.4",
    "@size-limit/preset-small-lib": "11.1.4",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/react-render-stream": "2.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/babel__preset-env": "^7.10.0",
    "@types/bytes": "3.1.4",
    "@types/fetch-mock": "7.3.8",
    "@types/jest": "29.5.12",
    "@types/jsesc": "^3.0.3",
    "@types/lodash": "4.17.7",
    "@types/node": "^22.10.7",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/relay-runtime": "14.1.24",
    "@types/use-sync-external-store": "0.0.6",
    "@typescript-eslint/eslint-plugin": "8.21.0",
    "@typescript-eslint/parser": "8.21.0",
    "@typescript-eslint/rule-tester": "8.21.0",
    "@typescript-eslint/types": "8.21.0",
    "@typescript-eslint/utils": "8.21.0",
    "ast-types": "0.16.1",
    "eslint": "9.18.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-local-rules": "3.0.2",
    "eslint-plugin-react-compiler": "19.0.0-beta-aeaed83-20250323",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-testing-library": "7.1.1",
    "expect": "29.7.0",
    "expect-type": "1.1.0",
    "fetch-mock": "9.11.0",
    "globals": "15.14.0",
    "graphql": "16.9.0",
    "graphql-17-alpha2": "npm:graphql@17.0.0-alpha.2",
    "graphql-ws": "6.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-matcher-utils": "29.7.0",
    "jsdom": "25.0.1",
    "jsesc": "^3.1.0",
    "knip": "^5.42.2",
    "lodash": "4.17.21",
    "madge": "^8.0.0",
    "patch-package": "8.0.0",
    "pkg-pr-new": "0.0.24",
    "prettier": "3.1.1",
    "publint": "^0.3.8",
    "react": "19.1.0",
    "react-17": "npm:react@^17",
    "react-18": "npm:react@^18",
    "react-dom": "19.1.0",
    "react-dom-17": "npm:react-dom@^17",
    "react-dom-18": "npm:react-dom@^18",
    "react-error-boundary": "4.0.13",
    "recast": "0.23.9",
    "rimraf": "5.0.9",
    "rxjs": "7.8.1",
    "size-limit": "11.1.4",
    "size-limit-apollo-plugin": "file:config/size-limit",
    "sorcery": "^1.0.0",
    "source-map": "^0.6.1",
    "sourcemap-validator": "^2.1.0",
    "subscriptions-transport-ws": "0.11.0",
    "terser": "5.31.3",
    "ts-api-utils": "2.0.0",
    "ts-jest": "29.2.3",
    "tsx": "4.19.2",
    "typedoc": "0.25.0",
    "typescript": "5.7.3",
    "web-streams-polyfill": "4.0.0",
    "zx": "^8.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "LICENSE",
    "**/*.md",
    "**/*.cjs",
    "**/*.cjs.map",
    "**/*.d.cts",
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts",
    "**/*.json"
  ],
  "overrides": {
    "pretty-format": "^29.7.0",
    "@testing-library/dom": "$@testing-library/dom"
  },
  "workspaces": [
    ".",
    "codegen"
  ]
}
