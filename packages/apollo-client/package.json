{
  "name": "apollo-client",
  "private": true,
  "version": "2.4.8",
  "description": "A simple yet functional GraphQL client.",
  "main": "./lib/index.js",
  "module": "./lib/bundle.esm.js",
  "browser": "./lib/bundle.umd.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn lint && yarn build",
    "coverage": "jest --coverage",
    "dev": "./scripts/dev.sh",
    "deploy": "./scripts/deploy.sh",
    "test": "tsc -p tsconfig.test.json --noEmit && jest",
    "benchmark": "yarn build:benchmark && node benchmark_lib/benchmark/index.js",
    "benchmark:inspect": "yarn build:benchmark && node --inspect --debug-brk benchmark_lib/benchmark/index.js",
    "filesize": "yarn minify",
    "type-check": "flow check",
    "prebuild": "rm -rf lib",
    "build": "tsc -b .",
    "build:benchmark": "tsc -p tsconfig.benchmark.json",
    "minify": "uglifyjs -c -m -o ./lib/bundle.min.js -- ./lib/bundle.umd.js",
    "watch": "tsc -w",
    "bundle": "rollup -c rollup.config.js",
    "postbuild": "yarn bundle",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts",
    "testonly": "jest",
    "preanalyze": "yarn build",
    "analyze": "webpack -p --config analyze/webpack.config.js",
    "clean": "rm -rf coverage/* && rm -rf lib/*"
  },
  "repository": {
    "type": "git",
    "url": "apollographql/apollo-client"
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "@types/zen-observable": "^0.8.0",
    "apollo-cache": "^1.1.22",
    "apollo-link": "^1.0.0",
    "apollo-link-dedup": "^1.0.0",
    "apollo-utilities": "^1.0.27",
    "symbol-observable": "^1.0.2",
    "zen-observable": "^0.8.0"
  },
  "devDependencies": {
    "webpack": "3.12.0",
    "webpack-bundle-analyzer": "3.0.3"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "optionalDependencies": {
    "@types/async": "2.0.50"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/scripts/tests.js"
    ],
    "testURL": "http://localhost"
  }
}
