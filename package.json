{
  "name": "apollo-client",
  "version": "0.0.1",
  "description": "A simple yet functional GraphQL client.",
  "main": "./lib/index.js",
  "scripts": {
    "pretest": "npm run compile",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --reporter spec --full-trace lib/test/tests.js",
    "posttest": "npm run lint",
    "install-typings": "./node_modules/.bin/typings i",
    "compile": "tsc",
    "prepublish": "npm run compile",
    "lint": "tslint src/*.ts && tslint test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "apollostack/apollo-client"
  },
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^3.1.2",
    "graphql": "^0.4.18",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.6.1",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "dataloader": "^1.1.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.3",
    "request-promise": "^2.0.1",
    "source-map-support": "^0.4.0",
    "tslint": "^3.6.0",
    "typescript": "^1.8.9",
    "typescript-require": "^0.2.9-1",
    "typings": "^0.7.9"
  }
}
