{
  "name": "apollo-client",
  "version": "0.0.1",
  "description": "Compile your Relay GraphQL queries at runtime, no need for Babel. Only for use in development.",
  "main": "./lib/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "pretest": "npm run compile",
    "test": "mocha --reporter spec --full-trace test/index.js",
    "posttest": "npm run lint",
    "postinstall": "./node_modules/.bin/typings i",
    "compile": "tsc",
    "prepublish": "npm run compile",
    "lint": "tslint src/*.ts && tslint test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "meteor/relay-runtime-query"
  },
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "graphql": "^0.4.18",
    "lodash": "^4.6.1",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dataloader": "^1.1.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "lodash": "^4.5.1",
    "mocha": "^2.3.3",
    "request-promise": "^2.0.1",
    "tslint": "^3.6.0",
    "typescript": "^1.8.9",
    "typescript-require": "^0.2.9-1",
    "typings": "^0.7.9"
  },
  "eslintConfig": {
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0
    },
    "env": {
      "mocha": true
    }
  }
}
