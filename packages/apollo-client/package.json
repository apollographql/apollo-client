{
  "name": "apollo-client",
  "private": true,
  "version": "2.0.0-beta.4",
  "description": "A simple yet functional GraphQL client.",
  "main": "./lib/apollo.umd.js",
  "module": "./lib/index.js",
  "jsnext:main": "/lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "coverage": "jest --coverage",
    "dev": "./scripts/dev.sh",
    "deploy": "./scripts/deploy.sh",
    "test": "jest",
    "benchmark":
      "npm run build:benchmark && node --stack-size=20000 lib/benchmark/index.js",
    "benchmark:inspect":
      "npm run build:benchmark && node --stack-size=20000 --inspect --debug-brk lib/benchmark/index.js",
    "filesize": "npm run build && npm run build:browser",
    "type-check": "flow check",
    "build": "tsc",
    "build:benchmark": "tsc -p tsconfig.test.json",
    "build:browser":
      "browserify ./lib/apollo.umd.js -o=./lib/bundle.js --i apollo-utilities --i apollo-cache --i apollo-link && npm run minify:browser",
    "minify:browser":
      "uglifyjs -c -m -o ./lib/bundle.min.js -- ./lib/bundle.js",
    "watch": "tsc -w",
    "bundle": "rollup -c",
    "postbuild": "npm run bundle",
    "lint": "tslint --type-check -p tsconfig.json src/*.ts",
    "testonly": "jest",
    "preanalyze": "npm run build",
    "analyze": "webpack -p --config analyze/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "apollographql/apollo-client"
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript",
    "relay",
    "npm",
    "react"
  ],
  "author": "Sashko Stubailo <sashko@stubailo.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-cache": "^0.2.0-beta.2",
    "apollo-link": "0.7.0",
    "apollo-link-dedup": "0.5.0",
    "apollo-utilities": "^0.2.0-beta.2",
    "graphql": "^0.11.0",
    "symbol-observable": "^1.0.2"
  },
  "devDependencies": {
    "@types/benchmark": "1.0.30",
    "@types/graphql": "0.9.4",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "21.1.2",
    "@types/lodash": "4.14.77",
    "@types/node": "8.0.32",
    "apollo-cache-inmemory": "^0.2.0-beta.4",
    "benchmark": "2.1.4",
    "browserify": "14.4.0",
    "bundlesize": "0.15.1",
    "danger": "1.1.0",
    "flow-bin": "0.56.0",
    "graphql-tag": "2.4.2",
    "isomorphic-fetch": "2.2.1",
    "jest": "20.0.4",
    "lint-staged": "4.2.3",
    "lodash": "4.17.4",
    "rollup": "0.50.0",
    "rxjs": "5.4.2",
    "ts-jest": "20.0.14",
    "tslint": "5.5.0",
    "typescript": "2.4.2",
    "uglify-js": "3.1.2",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.8.3"
  },
  "optionalDependencies": {
    "@types/async": "2.0.43"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json"],
    "setupFiles": ["<rootDir>/scripts/tests.js"]
  }
}
